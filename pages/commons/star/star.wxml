<template name="StarTemplate">
  <view class='star-box'>
    <block wx:for="{{stars}}" wx:item="item" wx:key="i">
      <image wx:if="{{item == '1'}}" src="/image/icon/star.png"></image>
      <image wx:else src="/image/icon/none-star.png"></image>
    </block>
    <text class='score'>{{average}}</text>
  </view>
</template>